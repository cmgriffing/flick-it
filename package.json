{
  "name": "flick-it",
  "version": "0.0.42",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev pages",
    "build": "vitepress build pages",
    "serve": "vitepress serve pages",
    "deploy:config:copy": "mv ./pages/.vitepress/dist ./dist",
    "deploy:player:copy": "bash copy-player.sh",
    "deploy:gh-pages": "gh-pages -d dist",
    "deploy": "run-s  build deploy:config:copy deploy:player:copy deploy:gh-pages",
    "install:player": "yarn install --modules-folder ./player",
    "install:player": "yarn install"
    "setup": "run-s install:player install:player"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",    
    "npm-run-all": "^4.1.5",
    "vitepress": "^0.20.0"
  },
  "dependencies": {
    "element-plus": "^1.1.0-beta.24",
    "shepherd.js": "^8.3.1"
  }
}
